<body>
    <div class="main">
        <section class="sign-in">
            <div class="container">
                <div class="signin-content">
                    <div class="signin-image">
                        <figure><img src="~/colorlib-regform-7/images/signin-image.jpg" alt="sing up image"></figure>
                        <a asp-page="./Register" asp-route-returnUrl="@Model.ReturnUrl">Create an account</a>

                    </div>
                    <div class="signin-form">
                        <h2 class="form-title">Sign in</h2>
                        <form method="POST" class="register-form" id="login-form">
                            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                            <div class="form-group">
                                <label asp-for="Input.Email" for="your_name"><i class="zmdi zmdi-account material-icons-name"></i></label>
                                <input asp-for="Input.Email" autocomplete="username" aria-required="true" placeholder="Email">
                                <span asp-validation-for="Input.Email" class="text-danger"></span>

                            </div>
                            <div class="form-group">
                                <label asp-for="Input.Password" for="your_pass"><i class="zmdi zmdi-lock"></i></label>
                                <input asp-for="Input.Password" autocomplete="current-password" aria-required="true" placeholder="Password">
                                <span asp-validation-for="Input.Email" class="text-danger"></span>
                            </div>
                            <div class="form-group">
                                <input type="checkbox" name="remember-me" id="remember-me" class="form-check-input" asp-for="Input.RememberMe">
                                <label for="remember-me" asp-for="Input.RememberMe" class="label-agree-term"><span><span></span></span>@Html.DisplayNameFor(m => m.Input.RememberMe)</label>

                            </div>
                            <div class="form-group form-button">
                                <input type="submit" name="signin" id="signin" class="form-submit" value="Log in">
                            </div>

                            <p class="mb-1">
                                <a id="forgot-password" asp-page="./ForgotPassword">Forgot your password?</a>
                            </p>
                        </form>
                        <div class="social-login">
                            <span class="social-label">Or login with</span>
                            <ul class="socials">
                                <li>
                                    <a href=<hr />
                                        @{
                                            if ((Model.ExternalLogins?.Count ?? 0) != 0)
                                            {
                                    <form id="external-account" asp-page="./ExternalLogin" asp-route-returnUrl="@Model.ReturnUrl" method="post" class="form-horizontal">
                                        <div>
                                            <p>
                                                            @foreach (var provider in Model.ExternalLogins)
                                                            {
                                                            <button type="submit" class="btn btn-primary" name="provider" value="@provider.Name" title="Log in using your @provider.DisplayName account"><i class="display-flex-center zmdi zmdi-facebook"></i></button>
                                                            }
                                            </p>
                                        </div>
                                    </form>
                                            }
                                        }</a>
                                </li>
                                @*<li><a href="#"><i class="display-flex-center zmdi zmdi-google"></i></a></li>*@
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </div>

    <script src="~/colorlib-regform-7/vendor/jquery/jquery.min.js"></script>
    <script src="~/colorlib-regform-7/js/main.js"></script>
